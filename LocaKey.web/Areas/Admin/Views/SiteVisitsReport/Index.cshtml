@model IEnumerable<LocaKey.Core.ViweModel.SiteVisitsReportVM>

@{
    ViewData["Title"] = "Index";
}
@if (Model.Any())
{
    <div class="card card-custom gutter-b">
        <div class="card-header">
            <div class="card-title">
                <h3 class="card-label">
                    تقرير زيارات الموقع 
                 
                </h3>
            </div>
            <div class="card-toolbar">
                @*<a asp-action="Create" class="btn btn-success showcreate" data-toggle="modal">
                    <i class="flaticon2-cube"></i> انشاء تقرير 
                </a>*@
            </div>
        </div>
        <div class="card-body">


            <table class="table table-hover">
                <thead>
                    <tr>
                        <th>
                            @Html.DisplayNameFor(model => model.Id)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.site_visit)
                        </th>
                          <th>
                            @Html.DisplayNameFor(model => model.IpAddress)
                        </th>
                          <th>
                            @Html.DisplayNameFor(model => model.MacAddress)
                        </th>
                         
                        <th></th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.Id)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.site_visit)
                            </td>
                             <td>
                                @Html.DisplayFor(modelItem => item.IpAddress)
                            </td>
                             <td>
                                @Html.DisplayFor(modelItem => item.MacAddress)
                            </td>
                            <td>
         
                             
                            </td>
                        </tr>
                    }



                </tbody>
            </table>
        </div>

    </div>

}
else
{
    <div class="card-toolbar">
        <a asp-action="Create" class="btn btn-success showcreate" data-toggle="modal">
            <i class="flaticon2-cube"></i> Create Evant Category
        </a>
    </div>
    <hr />
    <hr />

    <div class="alert alert-warning">

        No Data !!
    </div>

}
<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog-centered modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header bg-success text-light justify-content-center">
                <h5 class="modal-title"></h5>
            </div>
            <div class="modal-body justify-content-center" id="myModalContent">

            </div>
            <div class="modal-footer">
                <button class="btn btn-secondary" style="width:20%" data-dismiss="modal">
                    Close
                </button>
            </div>
        </div>
    </div>
</div>